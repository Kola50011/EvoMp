{
  "version": "1.0.0",
  "compilerOptions": {

    "lib": ["scripthost", "es6", "es2016", "es2015", "es7" ],
    "module": "esnext",
    "noImplicitAny": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2017"
  },
  "typeAcquisition": {
    "enable": true
  },
  "awesomeTypescriptLoaderOptions": {
    "useCache": true,
    "cacheDirectory": ".cache"
  },
  "include": [
    "EvoMp/*/Client/*.ts"
  ]
}
